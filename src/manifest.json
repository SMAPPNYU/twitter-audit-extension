{
  "name": "Chrome Extension Webpack",
  "version": "0.1",
  "background": {
    "scripts": ["background.bundle.js"]
  },
  "content_scripts": [
    {
       "matches": ["https://*.twitter.com/*"],
      "js": ["content.bundle.js"]
    }
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
