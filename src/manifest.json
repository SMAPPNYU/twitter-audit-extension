{
  "name": "Extension Survey Study",
  "version": "0.1",
  "background": {
    "service_worker": "background.bundle.js"
  },
  "content_scripts": [
    {
       "matches": ["https://*.twitter.com/*"],
      "js": ["content.bundle.js"]
    }
  ],
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "128": "icon128.png"
  },
  "host_permissions": [
      "https://www.example.com/*"
  ]
}
