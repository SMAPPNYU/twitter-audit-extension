{
  "name": "Extension Survey Study",
  "version": "0.1.4",
  "content_scripts": [
    {
       "matches": ["https://*.twitter.com/*", "http://*.twitter.com/*", "https://*.facebook.com/*"],
      "js": ["content.bundle.js"]
    }
  ],
  "background": {
    "service_worker": "background.bundle.js"
  },
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "notifications"
  ],
  "icons": {
    "128": "icon128.png"
  }
}
