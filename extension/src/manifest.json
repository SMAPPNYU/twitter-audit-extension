{
  "name": "Twitter Audit Extension Study",
  "version": "0.0.1",
  "content_scripts": [
    {
       "matches": ["https://*.twitter.com/*", "http://*.twitter.com/*"],
      "js": ["content.bundle.js"],
      "css": ["content.css"]
    }
  ],
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "128": "icon128.png"
  }
}
